{
  "name": "@trixta/phoenix-to-redux",
  "version": "1.0.0-beta.2",
  "description": "Middleware for react-redux to map Redux Actions and Phoenix Channel Message Events",
  "main": "lib/bundle.cjs.min.js",
  "module": "lib/bundle.esm.min.js",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trixtateam/phoenix-to-redux.git"
  },
  "keywords": [
    "phoenix",
    "phoenix-js",
    "phoenix-channels",
    "channels",
    "react",
    "redux",
    "websocket",
    "middleware",
    "channel",
    "message",
    "action"
  ],
  "author": "jacqueswho <jacqueswho@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trixtateam/phoenix-to-redux/issues"
  },
  "homepage": "https://github.com/trixtateam/phoenix-to-redux#readme",
  "scripts": {
    "build": "rollup -c",
    "prebuild": "rimraf lib",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --ignore-path .gitignore . --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "prettify": "prettier --write",
    "prepare": "npm run build"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "engines": {
    "node": ">=6"
  },
  "peerDependencies": {
    "lodash": "4.17.15",
    "phoenix": "1.3.4",
    "reselect": "4.0.0",
    "connected-react-router": "4.5.0"
  },
  "dependencies": {
    "lodash": ">=4",
    "phoenix": "1.3.4",
    "reselect": "4.0.0",
    "connected-react-router": "4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "babel-eslint": "10.1.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "eslint-plugin-redux-saga": "1.1.3",
    "husky": "^4.2.5",
    "pre-commit": "1.2.2",
    "prettier": "1.19.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.16.1",
    "rollup-plugin-terser": "^6.1.0"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib"
  }
}
